                                    },
                                    "cpf": {
                                        "type": "string"
                                    },
                                    "telefone1": {
                                        "type": "string"
                                    },
                                    "telefone2": {
                                        "type": "string"
                                    },
                                    "celular": {
                                        "type": "string"
                                    },
                                    "contato": {
